#include<stdio.h>
int main(){
        int i,a, n;
        printf("enter the number:");
        scanf("%d",&n);
        int sum=0;
        for(int i=1;i<=n;i++)
        {
               a=i*i;
               sum+=a;
         }
        printf("the sum of the natural numbers are:%d",sum);
        return 0;
}
